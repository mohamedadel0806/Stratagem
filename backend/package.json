{
  "name": "grc-platform-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:cov": "jest --config jest.config.js --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --config jest.config.js --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:governance": "jest --config jest.config.js --testPathPattern=governance",
    "migrate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/data-source.ts",
    "seed": "ts-node -r tsconfig-paths/register ./src/scripts/seed.ts",
    "seed:governance": "ts-node -r tsconfig-paths/register ./src/scripts/seed-governance.ts",
    "seed:governance:metrics": "ts-node -r tsconfig-paths/register ./src/scripts/seed-governance-metrics.ts",
    "seed:rules": "ts-node -r tsconfig-paths/register ./src/scripts/seed-validation-rules.ts",
    "seed:risks": "ts-node -r tsconfig-paths/register ./src/scripts/seed-risks.ts",
    "seed:kris": "ts-node -r tsconfig-paths/register ./src/scripts/seed-kris.ts",
    "seed:assessment-requests": "ts-node -r tsconfig-paths/register ./src/scripts/seed-assessment-requests.ts",
    "seed:report-templates": "ts-node -r tsconfig-paths/register ./src/scripts/seed-report-templates.ts",
    "check:governance": "ts-node -r tsconfig-paths/register ./src/scripts/check-governance-data.ts",
    "fix:policies-schema": "ts-node -r tsconfig-paths/register ./src/scripts/fix-policies-schema.ts",
    "test:signature": "ts-node -r tsconfig-paths/register ./src/scripts/test-digital-signature.ts",
    "validate:modules": "ts-node -r tsconfig-paths/register ./src/scripts/validate-modules.ts",
    "prebuild": "rimraf dist"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.12.2",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/cache-manager": "^2.2.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.3",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^10.0.2",
    "@types/multer": "^2.0.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "cache-manager": "^5.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "dotenv": "^17.2.3",
    "ioredis": "^5.3.2",
    "multer": "^2.0.2",
    "neo4j-driver": "^5.17.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-keycloak": "0.0.7",
    "pg": "^8.11.3",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^3.15.9",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
